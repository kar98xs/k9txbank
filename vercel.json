{
  "version": 2,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/app/?.*",
      "dest": "index.html"
    },
    {
      "src": "/login/?",
      "dest": "index.html"
    },
    {
      "src": "/register/?",
      "dest": "index.html"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ],
  "cleanUrls": true
}
