{
  "version": 2,
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "^/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/app(/.*)?$",
      "dest": "/index.html"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
}
